
@{
    ViewBag.Title = "Perfil";
    Layout = "~/Views/Shared/_LayoutInterno.cshtml";
}

<section class="page-section bg-light" id="team">
    <div class="container">
        <div class="text-center">
            <h2 class="section-heading text-uppercase">Perfil de usuario</h2>
            <h3 class="section-subheading text-muted">Conoce un poco mas de mi</h3>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="team-member">
                    <img class="mx-auto rounded-circle" src="~/WebStyles/assets/img/team/1.jpg" alt="..." />
                    <h4>@Session["NombreUsuario"].ToString()</h4>
                    <p class="text-muted">@Session["RolUsuario"].ToString()<br>@Session["CorreoUsuario"].ToString()</p>
                </div>
            </div>

        </div>
        <div style="text-align:center">
            <a href="" type="button" class="btn btn-primary btn-xl text-uppercase">Cambiar Contraseña</a>
        </div>
       
    </div>
</section>
